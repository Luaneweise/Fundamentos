#include <iostream>

using namespace std;

int main()
{
    float parcial1, parcial2, parcial3,examenfinal, proyectofinal, Notafinal; 
    cout <<"Ingresar primer parcial 1: ";
    cin >> parcial1;
    cout <<"Ingresar primer parcial 2: ";
    cin >> parcial2;
    cout <<"Ingresar primer parcial 3: ";
    cin >> parcial2;
    cout <<"Ingresar examen final: ";
    cin >>examenfinal;
    cout <<"Ingresar proyecto final: ";
    cin >>proyectofinal;
    Notafinal=((parcial1+parcial2+parcial3)/3)*0.40+examenfinal*0.40+proyectofinal*0.20;
    cout<<"La nota final es: "<<Notafinal;
    if (Notafinal>=51)
      cout<<"Esta aprobado ";
    else
      cout<< "Reprobo la materia ";
    return 0;
}
